<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CdController</servlet-name>
  	<display-name>CdController</display-name>
  	<description></description>
  	<servlet-class>com.controller.CdController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InternationalCDController</servlet-name>
  	<display-name>InternationalCDController</display-name>
  	<description></description>
  	<servlet-class>com.controller.InternationalCDController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegionalCdController</servlet-name>
  	<display-name>RegionalCdController</display-name>
  	<description></description>
  	<servlet-class>com.controller.RegionalCdController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CdController</servlet-name>
  	<url-pattern>/controller.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InternationalCDController</servlet-name>
  	<url-pattern>/controller2.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegionalCdController</servlet-name>
  	<url-pattern>/controller3.do</url-pattern>
  </servlet-mapping>
